<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_professor_list"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.zc.pickuplearn.ui.group.widget.prodetail.view.ProfessorListActivity">
    <com.zc.pickuplearn.ui.view.HeadView
        android:id="@+id/hv_list_head"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <com.github.jdsjlzx.recyclerview.LRecyclerView
        android:id="@+id/lrv_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
</LinearLayout>
